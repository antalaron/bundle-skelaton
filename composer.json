{
    "name": "antalaron/bundle-skelaton",
    "type": "symfony-bundle",
    "description": "Bundle skelaton for my Symfony bundles",
    "keywords": ["symfony-bundle", "bundle", "skelaton"],
    "homepage": "https://github.com/antalaron/bundle-skelaton",
    "license": "MIT",
    "authors": [
        {
            "name": "Antal Áron",
            "email": "antalaron@antalaron.hu",
            "homepage": "https://antalaron.hu"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "~2.7|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "<6.0.0",
        "friendsofphp/php-cs-fixer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Antalaron\\Bundle\\DemoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Antalaron\\Bundle\\DemoBundle\\Tests\\": "tests/Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
